,        "summary": "Dar like a un post",        "description": "Dar like a un post",        "operationId": "darLike",        "produces":[          "application/json"        ],        "parameters": [          {            "in": "path",            "name": "post_id",            "description": "id del post que se va a like",            "required": true,            "type": "integer"          },          {            "in": "path",            "name": "user_id",            "description": "ID del user que va a likear",            "required": true,            "type": "integer"          },          {            "in": "header",            "name": "token",            "description": "token",            "required": true,            "type": "string"          }        ],        "responses": {          "201": {            "description": "Interaccion creada con exito",            "schema": {              "$ref": "#/definitions/Likes"            }          },          "401": {            "description": "No autorizado (token inválido o faltante",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          },          "500": {            "description": "Error en el Servidor",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          },          "404": {            "description": "No se encontró al usuario",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          }        },        "security": [          {            "bearerAuth": []          }        ]      }    },    "/likes/delete/{user_id}/{post_id}": {      "delete": {        "tags": [          "Likes"        ],        "summary": "Dar dislike",        "description": "Servicio para quitar el comentario de una publicacion.",        "operationId": "dislikePost",        "produces": [            "application/json"        ],        "parameters": [          {            "in": "header",            "name": "token",            "description": "token",            "required": true,            "type": "string"          },          {            "in": "path",            "name": "user_id",            "description": "user_id",            "required": true,            "type": "integer"          },          {            "in": "path",            "name": "post_id",            "description": "id del post donde se quiere eliminar el like",            "required": true,            "type": "integer"          }        ],        "responses": {          "200": {            "description": "like eliminado con exito",            "schema": {              "$ref": "#/definitions/Likes"            }          },          "401": {            "description": "No autorizado (token inválido o faltante",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          },          "500": {            "description": "Error en el Servidor",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          },          "400": {            "description": "Solicitud invalida",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          }        },        "security": [          {            "bearerAuth": []          }        ]      }    },    "/comments/{post_id}": {      "get": {        "tags": [          "Comentarios"        ],        "summary": "Obtener los comentarios de un post",        "description": "Obtener los likes de un post",        "operationId": "obtenerLikes",        "produces":[          "application/json"        ],        "parameters": [          {            "in": "header",            "name": "token",            "description": "token para autenticación",            "required": true,            "type": "string"          },          {            "in": "path",            "name": "post_id",            "description": "post del que se obtendran los comentarios",            "required": true,            "type": "integer"          }        ],        "responses": {          "200": {            "description": "Lista de comentarios obtenida con exito",            "schema": {              "$ref": "#/definitions/Likes"            }          },          "401": {            "description": "No autorizado (token inválido o faltante",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          },          "500": {            "description": "Error en el Servidor",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          }        },        "security": [          {            "bearerAuth": []          }        ]      }    },    "/comments/create/{user_id}/{post_id}": {      "post": {        "tags": [          "Comentarios"        ],        "summary": "comentar un post",        "description": "comentar un post",        "operationId": "commentPost",        "produces":[          "application/json"        ],        "parameters": [          {            "in": "path",            "name": "user_id",            "description": "id del user que va a comentar",            "required": true,            "type": "integer"          },          {            "in": "path",            "name": "post_id",            "description": "ID del post al que se va a comentar",            "required": true,            "type": "integer"          },          {            "in": "body",            "name": "comment_text",            "description": "Comentario del post",            "required": true,            "schema":{              "type": "object",              "properties": {                "comment_text": {                  "type": "string",                  "description": "Comentario a subir"                }              },              "required": [                "comment_post"              ]            }          },          {            "in": "header",            "name": "token",            "description": "token",            "required": true,            "type": "string"          }        ],        "responses": {          "200": {            "description": "Interaccion creada con exito",            "schema": {              "$ref": "#/definitions/Likes"            }          },          "401": {            "description": "No autorizado (token inválido o faltante",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          },          "500": {            "description": "Error en el Servidor",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          },          "404": {            "description": "No se encontró al usuario",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          }        },        "security": [          {            "bearerAuth": []          }        ]      }    },    "/comments/delete/{user_id}/{id_comments}": {      "delete": {        "tags": [          "Comentarios"        ],        "summary": "Dar dislike",        "description": "Servicio para quitar el comentario de una publicacion.",        "operationId": "dislikePost",        "produces": [            "application/json"        ],        "parameters": [          {            "in": "header",            "name": "Authorization",            "description": "token",            "required": true,            "type": "string"          },          {            "in": "path",            "name": "user_id",            "description": "user_id",            "required": true,            "type": "integer"          },          {            "in": "path",            "name": "id_comments",            "description": "comentario a eliminar",            "required": true,            "type": "integer"          }        ],        "responses": {          "200": {            "description": "Comentario eliminado con exito",            "schema": {              "$ref": "#/definitions/Likes"            }          },          "401": {            "description": "No autorizado (token inválido o faltante",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          },          "500": {            "description": "Error en el Servidor",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          },          "400": {            "description": "Solicitud invalida",            "schema": {              "$ref": "#/definitions/ErrorResponse"            }          }        },        "security": [          {            "bearerAuth": []          }        ]      }    }  },  "definitions": {    "Likes": {      "type": "object",      "properties": {        "success": {            "type": "boolean",            "default": false        },        "message": {            "type": "string",            "example": "ok"        },        "data": {          "type": "string",          "example": "{}"        },        "status": {          "type": "string",          "example": "200"        }      }   },    "ErrorResponse": {      "type": "object",      "properties": {        "message": {          "type": "string",          "description": "Mensaje de error"        },        "errors": {          "type": "object",          "description": "Diccionario con los errores de validación"        }      }    },    "UpdatePostRequest": {      "type": "object",      "properties": {        "post_content": {          "type": "string",          "description": "contenido nuevo"        }      },      "required": [        "post_content"      ]    }  },  "securityDefinitions": {    "bearerAuth": {      "type": "apiKey",      "name": "